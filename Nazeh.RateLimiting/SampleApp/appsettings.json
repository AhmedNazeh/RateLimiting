{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Nazeh.RateLimiting": "Information"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "Enabled": true,
    "GlobalLimit": {
      "PermitLimit": 100,
      "Window": "00:01:00",
      "QueueLimit": 0
    },
    "ApiLimit": {
      "PermitLimit": 2,
      "Window": "00:01:00",
      "QueueLimit": 0
    },
    "AuthenticationLimit": {
      "PermitLimit": 5,
      "Window": "00:01:00",
      "QueueLimit": 0
    },
    "IpLimit": {
      "PermitLimit": 3,
      "Window": "00:01:00",
      "QueueLimit": 0,
      "SegmentSize": 10
    }
  }
}
